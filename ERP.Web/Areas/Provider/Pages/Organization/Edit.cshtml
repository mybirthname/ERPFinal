@page
@model ERP.Web.Areas.Provider.Pages.Organization.EditModel

@{
    ViewData["Title"] = "Edit";
}

@inject IStringLocalizer<SharedResources> sharedLocalizer

<nav class="navbar navbar-light" style="background-color: #e3f2fd; margin-bottom:15px;">
    <div class="navbar-brand">@sharedLocalizer["Organization"] </div>
</nav>

<div class="row">
    <div class="col-md-4">
        <form method="post">
            @Html.EditorForModel("OrganizationEdit")

            <div class="form-group">
                <a class="btn btn-light" asp-page="./Index"> @sharedLocalizer["BackToList"]</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}


