@inject SignInManager<User> SignInManager
@{
    var hasExternalLogins = (await SignInManager.GetExternalAuthenticationSchemesAsync()).Any();
}

@inject IStringLocalizer<SharedResources> sharedLocalizer


<ul class="nav nav-pills nav-stacked">
    <li class="@ManageNavPages.ChangePasswordNavClass(ViewContext)"><a id="change-password" asp-page="./ChangePassword">@sharedLocalizer["Password"]</a></li>

    <li class="@ManageNavPages.PersonalDataNavClass(ViewContext)"><a asp-page="./PersonalData">@sharedLocalizer["PersonalData"]</a></li>
</ul>
